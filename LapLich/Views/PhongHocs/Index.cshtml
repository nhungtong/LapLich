@model IEnumerable<LapLich.Models.PhongHoc>

@{
    ViewBag.Title = "Index";
}
<link href="~/Content/css/indexMH.css" rel="stylesheet" />
<h1>THÔNG TIN PHÒNG HỌC</h1>
<div class="content-area"
    <p>
        <a href="@Url.Action("Create", "PhongHocs")">CREATE NEW</a>

    </p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MaPhong)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TenPhong)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DayNha)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MaPhong)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TenPhong)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DayNha)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.MaPhong }) |
            @Html.ActionLink("Details", "Details", new { id=item.MaPhong }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.MaPhong })
        </td>
    </tr>
}

</table>
    </div>
